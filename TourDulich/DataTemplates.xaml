<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TourDulich.ViewModel"
                    xmlns:v="clr-namespace:TourDulich.View">
    <!--Admin Page-->
    <DataTemplate DataType="{x:Type vm:AdminViewModel}">
        <v:AdminView/>
    </DataTemplate>

    <!--Tour Page-->
    <DataTemplate DataType="{x:Type vm:TourViewModel}">
        <v:TourView/>
    </DataTemplate>

    <!--Cost Page-->
    <DataTemplate DataType="{x:Type vm:CostViewModel}">
        <v:CostView/>
    </DataTemplate>
</ResourceDictionary>